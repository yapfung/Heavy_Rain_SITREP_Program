    def configure(self, event):
        '''Save user entries in the initial file for subsequent processes'''
        try:
            import ConfigParser
            Config = ConfigParser.ConfigParser()
            Config.add_section('Date')
            Config.set('Date', 'date', self.Entry4.get())
            Config.add_section('Rain_Onset')
            Config.set('Rain_Onset', 'rain_onset', self.Entry1.get())
            Config.add_section('Period_of_Heaviest_Rain')
            Config.set('Period_of_Heaviest_Rain', 'heaviest_start', self.Entry2.get())
            Config.set('Period_of_Heaviest_Rain', 'heaviest_end', self.Entry3.get())
            Config.add_section('Affected_Areas')
            Config.set('Affected_Areas', 'north', heavy_rain_sitrep_gui_support.north.get())
            Config.set('Affected_Areas', 'south', heavy_rain_sitrep_gui_support.south.get())
            Config.set('Affected_Areas', 'east', heavy_rain_sitrep_gui_support.east.get())
            Config.set('Affected_Areas', 'west', heavy_rain_sitrep_gui_support.west.get())
            Config.set('Affected_Areas', 'central', heavy_rain_sitrep_gui_support.central.get())
            Config.set('Affected_Areas', 'many', heavy_rain_sitrep_gui_support.many.get())
            Config.set('Affected_Areas', 'most', heavy_rain_sitrep_gui_support.most.get())
            Config.add_section('Cause')
            Config.set('Cause', 'cause', self.Text1.get('1.0', 'end-1c'))
            Config.add_section('Flood_Reports')
            Config.set('Flood_Reports', 'flood_reports', self.Text2.get('1.0', 'end-1c'))
            Config.add_section('Forecast')
            Config.set('Forecast', 'forecast', self.Text3.get('1.0', 'end-1c'))
            Config.add_section('Remarks')
            Config.set('Remarks', 'remarks', self.Text4.get('1.0', 'end-1c'))
            Config.add_section('Past_3_Hours')
            Config.set('Past_3_Hours', 'past_3_hours', heavy_rain_sitrep_gui_support.past_3_hours.get())
            Config.add_section('Time_Filter')
            Config.set('Time_Filter', 'time_filter', heavy_rain_sitrep_gui_support.time_filter.get())
            Config.set('Time_Filter', 'filter_start', self.Entry5.get())
            Config.set('Time_Filter', 'filter_end', self.Entry6.get())
            Config.add_section('Issue_Time')
            Config.set('Issue_Time', 'issue_time', self.Entry7.get())
            Config.add_section('Excluded_Stations')
            Config.set('Excluded_Stations', 'excluded_stations', self.Text5.get('1.0', 'end-1c'))
            Config.add_section('SITREP_Criteria')
            Config.set('SITREP_Criteria', 'criteria_30min', self.Entry8.get())
            Config.set('SITREP_Criteria', 'criteria_60min', self.Entry9.get())
            Config.set('SITREP_Criteria', 'criteria_wind_gusts', self.Entry10.get())
            Config.add_section('Ad-Hoc_Request')
            Config.set('Ad-Hoc_Request', 'adhoc_request', heavy_rain_sitrep_gui_support.adhoc_request.get())
            Config.add_section('Url_Path')
            Config.set('Url_Path', 'url_daily_rainfall', self.Entry11.get())
            Config.set('Url_Path', 'url_60min_rainfall', self.Entry12.get())
            Config.set('Url_Path', 'url_60min_rainfall_past_3hrs', self.Entry13.get())
            Config.set('Url_Path', 'url_30min_rainfall', self.Entry14.get())
            Config.set('Url_Path', 'url_30min_rainfall_past_3hrs', self.Entry15.get())
            Config.set('Url_Path', 'url_wind_gusts', self.Entry16.get())
            Config.set('Url_Path', 'path_monthly_statistics', self.Entry17.get())
            with open('heavy_rain_sitrep.ini', "wb") as config_file:
                Config.write(config_file)
            # To check if user input the correct format for date and time
            dt.datetime.strptime(self.Entry4.get().rstrip(), '%d%m%y')
            dt.datetime.strptime(self.Entry1.get().rstrip(), '%d%m%y %H%M')
            dt.datetime.strptime(self.Entry2.get().rstrip(), '%d%m%y %H%M')
            dt.datetime.strptime(self.Entry3.get().rstrip(), '%d%m%y %H%M')
            if heavy_rain_sitrep_gui_support.time_filter.get() == '1':
                dt.datetime.strptime(self.Entry5.get().rstrip(), '%d%m%y %H%M')
                dt.datetime.strptime(self.Entry6.get().rstrip(), '%d%m%y %H%M')
            dt.datetime.strptime(self.Entry7.get().rstrip(), '%d%m%y %H%M')
        except Exception as e:
            print 'ERROR: Unable to configure user defined parameters'
            print e
        else:
            print 'INFO: User defined parameters configured'
            
    def generate_sitrep(self, event):
        '''Generate SITREP'''
        html_data.read_configuration()
        rainfall_daily = html_data.get_daily_rainfall()
        rainfall_60min = html_data.get_60min_rainfall()
        rainfall_30min = html_data.get_30min_rainfall()
        wind_gusts = html_data.get_wind_gusts()
        sitrep_generation.read_configuration()
        sitrep_generation.create_sitrep(rainfall_daily, rainfall_60min, rainfall_30min, wind_gusts)
        sitrep_generation.append_monthly_statistics()


