def vp_start_gui():
    '''Starting point when module is the main routine.'''
    os.system('mode con: cols=110 lines=30') # Additional customisations
    global val, w, root
    root = Tk()
    heavy_rain_sitrep_gui_support.set_Tk_var()
    top = New_Toplevel_1 (root)
    heavy_rain_sitrep_gui_support.init(root, top)
    top.enable_entry() # Additional customisations
    top.Button1.bind('<Button-1>', top.configure) # Additional customisations
    top.Button2.bind('<Button-1>', top.generate_sitrep) # Additional customisations
    root.title('Heavy Rain SITREP') # Additional customisations
    print heavy_rain_sitrep_gui_support.startup_message.get() # Additional customisations
    try: # Additional customisations
        root.iconbitmap('heavy_rain_sitrep_icon.ico')
        print heavy_rain_sitrep_gui_support.icon_credit.get()
    except:
        pass
    root.mainloop()

